package ver3_configuration;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;

@Configuration
public class Beans {
	@Bean
	public Car sonata() {
		Car c = new Car();
		c.setTire(koreaTire());
	}
	@Bean
	public Tire koreaTire() {
		
	}
	@Bean
	public Tire chinaTire() {
		
	}
}
